  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremy-faton/with-props.git"
  },
  "keywords": [
    "Higher Order Component",
    "React",
    "HOC",
    "TypeScript"
  ],
  "author": "Jeremy Faton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeremy-faton/with-props/issues"
  },
  "homepage": "https://github.com/jeremy-faton/with-props#readme",
  "peerDependencies": {
    "react": "^16 || ^17 || ^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "babel-jest": "^29.4.2",
    "eslint": "^8.33.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.4.2",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.14.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.2"
  },
  "files": [
    "dist/**/*.js",
    "dist/*.d.ts"
